{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  // Componentes do footer do site. Ambos utilizam o mesmo layout
  "footer-layout.desktop": { 
    "children": [
      "app-login",
      "app-home",
      "app-whatsapp"
    ],
    "props": {
